{
  "name": "oracle-server-ui-proxy",
  "version": "1.9.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitcoin-s/bitcoin-s-ts.git",
    "directory": "oracle-server-ui-proxy"
  },
  "scripts": {
    "build": "node --loader ts-node/esm --es-module-specifier-resolution=node ./build/buildVersion.ts && ./fixup.sh && cp config.json build.json ./dist/cjs && cp config.json build.json ./dist/mjs",
    "start": "node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-fetch server.ts",
    "startlocal": "DEFAULT_UI_PASSWORD=none BITCOIN_S_ORACLE_RPC_PASSWORD=password node --loader ts-node/esm --es-module-specifier-resolution=node --experimental-fetch server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack"
  },
  "license": "MIT",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "import": "./dist/mjs/*",
      "require": "./dist/cjs/*"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/mjs/*"
      ]
    }
  },
  "dependencies": {
    "@bitcoin-s-ts/oracle-server-ts": "file:../oracle-server-ts",
    "common-ts": "file:../common-ts",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "jsonwebtoken": "^8.5.1",
    "socks-proxy-agent": "^6.2.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.11.64",
    "git-last-commit": "^1.0.1",
    "nodemon": "^2.0.20",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  }
}
